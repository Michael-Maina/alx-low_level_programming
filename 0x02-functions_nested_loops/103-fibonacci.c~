#include <stdio.h>

/**
 * main - program starts here
 *
 * Return: prints sum of even numbered fibonacci numbers less than 4,000,000
 */

int main(void)
{
	int n;
	long int a = 1;
	long int b = 2;
	long int c;
	long int sum;

        for (n = 0; n < 50; n++)
	{
		c = a + b;
		a = b;
		b = c;

		if (c != 4,000,000)
			sum += c;
		else
			break;
	}

	printf("%d\n", sum);
	return (0);
}
