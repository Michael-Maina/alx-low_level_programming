#include <stdio.h>

/**
 * main - program starts here
 *
 * Return: prints the first 50 fibonacci numbers, starting with 1 and 2
 */

int main(void)
{
	int n;
	int a = 1;
	int b = 2;
	int c;

	printf("%d, %d ,", a, b);

	for (n = 1; n = 50; n++)
	{
		c = a + b;
		printf("%d, ", c);
		a = b;
		b = c;
	}

	printf("\n");
}
